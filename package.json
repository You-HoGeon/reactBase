{
  "name": "reactbase",
  "version": "1.0.0",
  "description": "A sceleton code for react using parcel.",
  "scripts": {
    "start": "npx parcel src/index.html",
    "build": "npx parcel build src/index.html --no-source-maps -d build"
  },
  "author": "YouHoGeon",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.23.7"
  },
  "browserslist": [
    "> 0.5%",
    "ie 9-11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "corejs": 3,
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {}
  }
}
